link(rel="stylesheet", href="../public/css/registrar-club-completo.css")
<link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.5.0/css/bootstrap.min.css" integrity="sha384-9aIt2nRpC12Uk9gS9baDl411NQApFmC26EwAOH8WgZl5MYYxFfc+NcPb1dKGj7Sk" crossorigin="anonymous">
     
<main>
    <div class="contenedorAlertas">
    </div>
    <div class="contenedorFormulario">

        <div class="formularioClubCompleto">    
            <form>
                <div class="contenedorCompletoImagen rounded-circle">
                    <div class="contenedorImagen">
                        img( id="img-preview" class="rounded-circle")
                    </div>
                    <div class="imagen">
                        input(type="file", name="imagen" id="imagen" )
                        <i class="fas fa-camera fa-2x">
                        </i>
                    </div>
                </div>

                <div class="contenedorFormulariocompletoClub">
                    <div class="form-group">
                        
                        <select id="disabledSelect" class="form-control">
                            <option>Seleccionar Club</option>
                        </select>
                    </div>
                    <div class="form-group">
                        input(type="tel", name="telefono" placeholder="Telefono" class="form-control")
                    </div>
                    <div class="form-group">
                        
                        <select id="disabledSelect" class="form-control">
                            <option>Seleccionar Estado</option>
                        </select>
                    </div>
                     <div class="form-group">
                        input(type="text", name="ciudad" placeholder="ciudad" class="form-control")
                    </div>

                    <div class="form-group">
                        input(type="submit", value="Registrar" class="form-control")
                    </div>
                    

                </div>
            </form>


        </div>  
    
    </div>
    













</main>

<script src="https://code.jquery.com/jquery-3.5.1.slim.min.js" integrity="sha384-DfXdz2htPH0lsSSs5nCTpuj/zy4C+OGpamoFVy38MVBnE+IbbVYUew+OrCXaRkfj" crossorigin="anonymous"></script>
